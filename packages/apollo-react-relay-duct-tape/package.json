{
  "name": "@graphitation/apollo-react-relay-duct-tape",
  "description": "A compatibility wrapper that provides the react-relay API on top of Apollo Client.",
  "license": "MIT",
  "version": "1.0.1-alpha.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/graphitation.git",
    "directory": "packages/apollo-react-relay-duct-tape"
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "monorepo-scripts build",
    "lint": "monorepo-scripts lint",
    "test": "monorepo-scripts test",
    "types": "monorepo-scripts types",
    "just": "monorepo-scripts",
    "relay": "graphitation-compiler --printWatchQueries --schema ./src/__tests__/schema.graphql --src ./src --exclude src/storeObservation/*FieldPolicy.test.ts"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.15",
    "@graphitation/apollo-mock-client": "^0.10.2",
    "@graphitation/graphql-js-operation-payload-generator": "^0.8.2",
    "@graphitation/graphql-js-tag": "^0.8.2",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.3",
    "graphql": "^15.0.0",
    "monorepo-scripts": "*",
    "react": "^17.0.2",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-graphitation": "^1.0.0-alpha.3",
    "ts-expect": "^1.3.0",
    "typescript": "^4.5.0"
  },
  "peerDependencies": {
    "@apollo/client": "^3.3.0",
    "graphql": "^15.0.0",
    "react": "^17.0.2",
    "relay-compiler-language-graphitation": "^1.0.0-alpha.3",
    "typescript": "^4.5.0"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "lodash": "^4.17.15"
  },
  "sideEffects": false,
  "access": "public",
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "module": "./lib/index.mjs",
    "exports": {
      ".": {
        "import": "./lib/index.mjs",
        "require": "./lib/index.js"
      },
      "./lib/storeObservation/createImportDocumentsTransform": {
        "import": "./lib/storeObservation/createImportDocumentsTransform.mjs",
        "require": "./lib/storeObservation/createImportDocumentsTransform.js"
      }
    }
  }
}
