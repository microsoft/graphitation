{
  "name": "apollo-watch-fragments",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@graphitation/apollo-react-relay-duct-tape": "^1.0.0-alpha.1",
    "@graphitation/graphql-js-tag": "^0.5.3",
    "@graphql-tools/schema": "^8.2.0",
    "graphql": "^15.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "yarn generate-resolver-typings && concurrently 'yarn relay --watch' 'yarn serve'",
    "serve": "webpack serve",
    "build": "webpack",
    "relay": "graphitation-compiler --schema ./data/schema.graphql --src ./src --printWatchQueries",
    "generate-resolver-typings": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphitation/apollo-react-relay-duct-tape": "^0.7.2",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-resolvers": "^2.2.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.1.0",
    "@welldone-software/why-did-you-render": "^6.2.1",
    "concurrently": "^6.2.1",
    "graphql-relay": "^0.10.0",
    "relay-compiler": "^10",
    "relay-compiler-language-graphitation": "^1.0.0-alpha.1",
    "ts-expect": "^1.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  }
}
