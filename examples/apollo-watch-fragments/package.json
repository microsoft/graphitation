{
  "name": "apollo-watch-fragments",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "~3.6.0",
    "@graphitation/apollo-react-relay-duct-tape": "^1.0.6",
    "@graphitation/graphql-js-tag": "^0.9.0",
    "@graphql-tools/schema": "^9.0.19",
    "graphql": "^15.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "yarn generate-resolver-typings && concurrently 'yarn duct-tape-compiler --watch' 'yarn serve'",
    "serve": "webpack serve",
    "build": "webpack",
    "duct-tape-compiler": "duct-tape-compiler --schema ./data/schema.graphql --src ./src --emitQueryDebugComments",
    "generate-resolver-typings": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphitation/apollo-react-relay-duct-tape-compiler": "^1.2.0",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-resolvers": "^2.2.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@welldone-software/why-did-you-render": "^6.2.1",
    "concurrently": "^6.2.1",
    "graphql-relay": "^0.10.0",
    "ts-loader": "9.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.9.5",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  }
}
