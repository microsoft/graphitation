{
  "name": "supermassive-todomvc",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently 'yarn relay --watch' 'yarn serve --progress --color --mode=development'",
    "build": "webpack --mode production --config webpack.config.ts",
    "serve": "webpack serve --config webpack.config.ts",
    "extract-schema": "supermassive extract-schema ./src/schema/typeDefs.graphql",
    "relay": "relay-compiler --language graphitation --schema ./src/schema/typeDefs.graphql --src ./src"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@babel/runtime": "^7.12.0",
    "@graphitation/apollo-react-relay-duct-tape": "^0.7.0",
    "@graphitation/graphql-js-tag": "^0.8.0",
    "@graphitation/supermassive": "^0.5.2",
    "@graphitation/ts-transform-graphql-js-tag": "^0.4.0",
    "concurrently": "^6.5.1",
    "graphql": "^15.6.1",
    "graphql-tools": "^8.2.0",
    "html-webpack-plugin": "^5.3.2",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "relay-compiler": "^12.0.0",
    "relay-compiler-language-graphitation": "^0.8.1",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-graphql-loader": "^1.0.2",
    "zen-observable": "^0.8.15"
  },
  "resolutions": {
    "graphql": "^15.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/import-types-preset": "^2.1.7",
    "@graphql-codegen/typescript": "^2.3.0",
    "@statoscope/webpack-plugin": "^5.18.0",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.1",
    "graphql-let": "^0.18.5",
    "prettier": "^2.4.1"
  }
}
